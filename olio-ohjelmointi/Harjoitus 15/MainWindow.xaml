<Window x:Class="Harjoitus_15.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Harjoitus_15"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid RenderTransformOrigin="0.498,0.481" Margin="0,0,0.6,3">
        <Grid.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform Angle="-0.383"/>
                <TranslateTransform/>
            </TransformGroup>
        </Grid.RenderTransform>
        <Button x:Name="Button_auto" Content="Lisää Auto" HorizontalAlignment="Left" Margin="330,176,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.559,0.548" Click="Button_auto_Click"/>
        <Button x:Name="Button_kuorma_auto" Content="Lisää Kuorma-auto" HorizontalAlignment="Left" Margin="542,176,0,0" VerticalAlignment="Top" Width="75" Click="Button_kuorma_auto_Click"/>
        <TextBlock x:Name="Auto" HorizontalAlignment="Left" Margin="355,245,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top"/>
        <TextBlock x:Name="KuormaAuto" HorizontalAlignment="Left" Margin="567,245,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top"/>

    </Grid>
</Window>
